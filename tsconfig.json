{
  "compilerOptions": {
    "experimentalDecorators": true,
    "baseUrl": "./src/",
    "jsx": "react",
    "allowJs": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*"]
}
